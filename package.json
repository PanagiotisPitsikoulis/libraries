    "zod": "^3.24.2",
    "@tailwindcss/postcss": "^4.0.14",
    "tailwindcss": "^4.0.14"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@biomejs/biome": "^1.9.4",
    "@inquirer/prompts": "^7.4.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.0.7",
