{
  "name": "next-toolchain",
  "version": "0.30.0",
  "type": "module",
  "main": "./dist/ui/index.cjs",
  "module": "./dist/ui/index.js",
  "types": "./dist/ui/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./styles": "./dist/ui/styles/globals.css",
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    },
    "./scripts": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    }
  },
  "bin": {
    "next-toolchain-config-env": "dist/node/env-manager-next/env-cli.js",
    "next-toolchain-config-db": "dist/node/db-manager-next/db-cli.js",
    "next-toolchain-publish": "dist/node/package-publisher-next/publish-cli.js",
    "next-toolchain-version": "dist/node/package-publisher-next/version-cli.js"
  },
  "files": [
    "dist/ui",
    "dist/node",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "clean:repo": "git checkout --orphan temp_branch && git add -A && git commit -m 'Initial commit' && git branch -D main && git branch -m main && git push -f --set-upstream origin main",
    "prebuild": "bun run clean",
    "build:ui": "tsup --config tsup.config.ts",
    "build:node": "tsup --config tsup.node.config.ts",
    "build": "bun run prebuild && bun run build:node && bun run build:ui",
    "db": "node --experimental-specifier-resolution=node dist/node/db-manager-next/db-cli.js",
    "env": "node --experimental-specifier-resolution=node dist/node/env-manager-next/env-cli.js",
    "publish": "bun run build && bun ./src/scripts/package-publisher-next/publish-cli.ts",
    "publish:patch": "bun ./src/scripts/package-publisher-next/version-cli.ts patch && bun run publish",
    "publish:minor": "bun ./src/scripts/package-publisher-next/version-cli.ts minor && bun run publish",
    "publish:major": "bun ./src/scripts/package-publisher-next/version-cli.ts major && bun run publish",
    "version:patch": "bun ./src/scripts/package-publisher-next/version-cli.ts patch",
    "version:minor": "bun ./src/scripts/package-publisher-next/version-cli.ts minor",
    "version:major": "bun ./src/scripts/package-publisher-next/version-cli.ts major"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.5",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-portal": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/unist": "^3.0.3",
    "ai": "^4.1.61",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "color": "^5.0.0",
    "cross-env": "^7.0.3",
